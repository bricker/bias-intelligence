{
    "manifest_version": 2,
    "name": "Bias Intelligence",
    "version": "1.0.0",
    "content_scripts": [
      {
        "matches": [ "*" ],
        "js": ["src/index.js"]
      }
    ],
    "icons": {
      "16": "images/neutral-16.png"
    },
    "browser_action": {
      "default_icon": {
        "16": "images/neutral-16.png"
      }
    },
    "background": {
      "scripts": [ "background.js" ],
      "persistent": false
    },
    "permissions": [ "storage" ]
  }